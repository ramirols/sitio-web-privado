{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 20
    }
  },
  "builds": [
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}